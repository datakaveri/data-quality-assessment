{
    "$id": "https://voc.iudx.org.in/WasteManagement.jsonld",
    "$schema": "http://jsonschema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "deviceBatteryStatus": {
        "type": "string"
      },
      "wardID": {
        "type": "string"
      },
      "vehicleType": {
        "type": "string"
      },
      "license_plate": {
        "type": "string"
      },
      "bearing": {
        "type": "integer"
      },
      "speed": {
        "type": "integer"
      },
      "location": {
        "type": "object",
        "properties": {
          "coordinates": {
            "type": "array",
            "items": [
              {
                "type": "number"
              },
              {
                "type": "number"
              }
            ]
          },
          "type": {
            "type": "string"
          }
        },
        "required": [
          "coordinates",
          "type"
        ]
      },
      "ignitionStatus": {
        "type": "string"
      },
      "vehicleRunningStatus": {
        "type": "string"
      },
      "observationDateTime": {
        "type": "string",
        "format": "date-time"
      }
    },
    "additionalProperties": false,
    "required": [
      "deviceBatteryStatus",
      "wardID",
      "vehicleType",
      "license_plate",
      "bearing",
      "speed",
      "location",
      "ignitionStatus",
      "vehicleRunningStatus",
      "observationDateTime"
    ],
    "description": "JSON schema for real-time vehicle tracking data for the solid waste management domain."
  }